{
  "name": "Privacy Extension",
  "version": "0.5.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Privacy Graph",
    "default_popup": "/popup/popup.html"
  },
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "permissions": [
    "background",
    "webRequest",
    "storage",
    "unlimitedStorage",
    "downloads",
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "options_page": "index.html"
}